    "contentVersion": "1.0.0.0",
    "parameters": {
        "name": {
            "value": "${AZURE_ENV_NAME}"
        },
        "location": {
            "value": "${AZURE_LOCATION}"
        },
        "principalId": {
            "value": "${AZURE_PRINCIPAL_ID}"
        },
        "ApiPublisherName": {
            "value": "${ApiPublisherName=Placeholder Name}"
